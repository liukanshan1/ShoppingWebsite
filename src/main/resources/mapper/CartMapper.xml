<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.liukanshan.shoppingwebsite.mapper.CartMapper">
    <select id="selectByUserItem" resultType="top.liukanshan.shoppingwebsite.entity.Cart">
        SELECT *
        FROM  tb_cart
        WHERE user_id = #{userId} AND item_id = #{itemId}
    </select>

</mapper>