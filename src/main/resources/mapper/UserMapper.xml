<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.liukanshan.shoppingwebsite.mapper.UserMapper">
    <select id="selectUserByName" resultType="top.liukanshan.shoppingwebsite.entity.User">
        SELECT *
        FROM  tb_user
        WHERE username =  #{userName}
    </select>

</mapper>